{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "max-classes-per-file": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-var": "off",
    "wrap-iife": "off",
    "prefer-rest-params": "off"
  }
}
