{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "linebreak-style": "off",
    "max-classes-per-file": "off",
    "no-shadow": "off",
    "newline-per-chained-call": "off",
    "consistent-return": "off",
    "no-console": "off",
    "no-extend-native": "off",
    "no-plusplus": "off",
    "no-bitwise": "off"
  }
}
